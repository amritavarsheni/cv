#include <stdio.h>

int main()
{
    int a[100],n,i,ele;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    scanf("%d",&ele);
    for(i=0;i<n;i++)
    {
        if(a[i]==ele)
        {
            printf("\n Element %d is found at %d position",a[i],i+1);
        }
    }

    return 0;
}
