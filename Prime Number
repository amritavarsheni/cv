#include <stdio.h>

int main()
{
    int n,i,flag=0;
    scanf("%d",&n);
    for(i=2;i<n/2;i++)
    {
        if(i%n==0)
        {
            flag=1;
            break;
        }
    }
    if(flag)
    {
        printf("Prime Number");
    }
    else
    {
        printf("Not a Prime Number");
    }
    return 0;
}
